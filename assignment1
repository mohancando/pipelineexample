pipeline{
	agent any
	parameters {
        	string(name: 'BRANCH_NAME', defaultValue: 'branch1', description: 'Who should I say hello to?')
	}
	stages{
		stage('Build'){
			steps{
				//git branch: 'main', url: 'https://github.com/mohancando/pipelineexample.git'
				sh 'echo "Hello mohan"'
			}
		}
		
		stage('Test') {
            		steps {
                		sh 'git checkout ${params.BRANCH_NAME}; echo "hello params branch1"'
			
            		}
        	}
	}
}
